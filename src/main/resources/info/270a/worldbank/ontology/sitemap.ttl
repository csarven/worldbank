@prefix rdf:	<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:	<http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:	<http://www.w3.org/2001/XMLSchema#> .
@prefix owl:	<http://www.w3.org/2002/07/owl#> .
@prefix lda:	<http://purl.org/linked-data/api/vocab#> .
@prefix sp:	<http://spinrdf.org/sp#> .
@prefix spin:	<http://spinrdf.org/spin#> .
@prefix void:	<http://rdfs.org/ns/void#> .
@prefix ldp:	<http://www.w3.org/ns/ldp#> .
@prefix sioc:	<http://rdfs.org/sioc/ns#> .
@prefix foaf:	<http://xmlns.com/foaf/0.1/> .
@prefix gc:     <http://client.graphity.org/ontology#> .
@prefix gp:	<http://processor.graphity.org/ontology#> .
@prefix srv:    <http://jena.hpl.hp.com/Service#> .

<#> a owl:Ontology ;
    owl:imports gp:, gc:, lda: , void:, foaf: .

# RESOURCE CLASSES

<#Resource> a owl:Class, gp:Template ;
    rdfs:subClassOf foaf:Document, lda:ItemEndpoint ;
    lda:uriTemplate "/{path: .*}" ;
    spin:query [ a gp:DescribeFromGraphWithChildrenTemplate ] ;
    gp:limit 20 ;
    # gp:cacheControl "public, no-transform, max-age=21600" ;
    rdfs:label "Resource" ;
    rdfs:comment "Catch-all resource class (template)" ;
    rdfs:isDefinedBy <#> .

# RESOURCES

<.> a foaf:Document . # dummy